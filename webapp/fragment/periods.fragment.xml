<core:FragmentDefinition xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Panel expandable="true" expanded="false" width="auto">
		<headerToolbar>
			<Toolbar height="3rem">
				<Title text="01.01.2018 - 02.01.2018" />
				<ToolbarSpacer />
				<CheckBox select="onCheckBoxSelect" selected="false" />
			</Toolbar>
		</headerToolbar>
		<content>
			<VBox>
				<Label class="topLabel" text="{i18n>dateFrom}" />
				<DatePicker enabled="false" dateValue="{DateFrom}" />
				<Label class="topLabel" text="{i18n>dateTo}" />
				<DatePicker enabled="false" dateValue="{DateTo}" />
				<Label class="topLabel" text="{i18n>numberOfShipments}" />
				<Input type="Number" enabled="false" value="{NumberOfShipments}" />
				<Label class="topLabel" text="{i18n>shipmentSize}" />
				<Input type="Number" enabled="false" value="{ShipmentSize}" />
				<HBox justifyContent="SpaceBetween">
					<VBox class="leftVBoxStyle" width="50%">
						<Label class="topLabel" text="{i18n>shipmentTolerance}" />
						<Input type="Number" enabled="false" value="{ShipmentTolerance}" />
					</VBox>
					<VBox class="rightVBoxStyle" width="50%">
						<Label class="topLabel" text="{i18n>toleranceUom}" />
						<Select enabled="false" forceSelection="false" selectedKey="%" width="100%">
							<core:Item key="" text=""/>
							<core:Item key="%" text="%"/>
						</Select>
					</VBox>
				</HBox>
				
				<Label class="topLabel" text="{i18n>tonnage}" />
				<Input type="Number" enabled="false" value="{Tonnage}" />
				<HBox justifyContent="SpaceBetween">
					<VBox class="leftVBoxStyle" width="50%">
						<Label class="topLabel" text="{i18n>tonnageTolerance}" />
						<Input type="Number" enabled="false" value="10" />
					</VBox>
					<VBox class="rightVBoxStyle" width="50%">
						<Label class="topLabel" text="{i18n>toleranceUom}" />
						<Select enabled="false" forceSelection="false" selectedKey="%" width="100%">
							<core:Item key="" text=""/>
							<core:Item key="%" text="%"/>
						</Select>
					</VBox>
				</HBox>
			</VBox>
		</content>
	</Panel>
</core:FragmentDefinition>