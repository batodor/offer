<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>volume}" stretch="true" class="sapUiContentPadding" id="volumeDialog">
		<content>
			<Switch class="topLabel" custom:id="volumeOwner" change="onSwitch" state="false" customTextOn="{i18n>optional}" customTextOff="{i18n>fixed}">
				<layoutData>
					<FlexItemData growFactor="1" />
				</layoutData>
			</Switch>
			<ToolbarSpacer/>
			<Label required="true" class="topLabel" text="{i18n>number} (max: 36)" />
			<ToolbarSpacer/>
			<Input custom:key="number" custom:max="36" value="{Number}" liveChange="checkValue" type="Number" width="100%"/>
			<ToolbarSpacer/>
			<Label required="true" class="topLabel" text="{i18n>meansOfTransport}" />
			<ToolbarSpacer/>
			<Select forceSelection="false" selectedKey="" width="100%">
				<core:Item key="" text=""/>
				<core:Item key="vessel" text="{i18n>vessel}"/>
				<core:Item key="railway" text="{i18n>railway}"/>
			</Select>
			<ToolbarSpacer/>
			<Label required="true" class="topLabel" text="{i18n>owner}" />
			<ToolbarSpacer/>
			<Select enabled="false" id="volumeOwner" forceSelection="false" selectedKey="me" width="100%">
				<core:Item key="me" text="{i18n>myself}"/>
				<core:Item key="bp" text="{i18n>businessPartner}"/>
			</Select>
			
		</content>
		<buttons>
			<Button text="{i18n>cancel}" custom:id="volume" type="Back" press="dialogCancel"/>
			<Button text="{i18n>add}" custom:id="volume" visible="false" type="Accept" press="dialogAdd"/>
			<Button text="{i18n>save}" custom:id="volume" visible="false" type="Accept" press="dialogSave"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>