<core:FragmentDefinition xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Panel expandable="true" expanded="true" width="auto">
		<headerToolbar>
			<Toolbar height="3rem">
				<Title text="02" />
				<ToolbarSpacer />
				<CheckBox select="onCheckBoxSelect" custom:id="volume2" selected="false" />
			</Toolbar>
		</headerToolbar>
		<content>
			<VBox>
				<HBox justifyContent="SpaceBetween">
					<VBox class="leftVBoxStyle" width="50%">
						<Label class="topLabel" text="{i18n>type}" />
						<Text text="{i18n>optional}" />
					</VBox>
					<VBox class="rightVBoxStyle" width="50%">
						<Label class="topLabel" text="{i18n>owner}" />
						<Text text="Business Partner" />
					</VBox>
				</HBox>
				<Label class="topLabel" text="{i18n>meansOfTransport}" />
				<Text text="Vessel" />
			</VBox>
			<List noDataText="{i18n>noData}" selectionChange="onListSelect">
				<headerToolbar>
					<Toolbar>
						<Title text="{i18n>prices}" titleStyle="H3" />
						<ToolbarSpacer />
						<Button type="Accept" id="price2Add" enabled="false" custom:id="price2" text="{i18n>add}" press="add" />
						<Button type="Emphasized" custom:id="price2" text="{i18n>edit}" press="edit" enabled="false" />
						<Button type="Reject" custom:id="price2" text="{i18n>delete}" press="delete" enabled="false" />
					</Toolbar>
				</headerToolbar>
				<items>
					<CustomListItem>
						<core:Fragment fragmentName="fragment.prices2" type="XML"></core:Fragment>
					</CustomListItem>
				</items>
			</List>
			<List noDataText="{i18n>noData}" selectionChange="onListSelect">
				<headerToolbar>
					<Toolbar>
						<Title text="{i18n>periods}" titleStyle="H3" />
						<ToolbarSpacer />
						<Button type="Accept" id="period2Add" enabled="false" custom:id="period2" text="{i18n>add}" press="add" />
						<Button type="Emphasized" custom:id="period2" text="{i18n>edit}" press="edit" enabled="false" />
						<Button type="Reject" custom:id="period2" text="{i18n>delete}" press="delete" enabled="false" />
					</Toolbar>
				</headerToolbar>
				<items>
					<CustomListItem>
						<core:Fragment fragmentName="fragment.periods2" type="XML"></core:Fragment>
					</CustomListItem>
				</items>
			</List>
		</content>
	</Panel>
</core:FragmentDefinition>